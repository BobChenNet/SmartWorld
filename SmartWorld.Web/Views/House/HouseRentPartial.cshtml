
@model SmartWorld.Uitls.Result<List<SmartWorld.Model.RentHouse>>

<p>
    @Html.ActionLink("新建", "AddHouseInfo")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].HomeCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].HomeTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].Floor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].Text)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Data) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HomeCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HomeTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Floor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Text)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
